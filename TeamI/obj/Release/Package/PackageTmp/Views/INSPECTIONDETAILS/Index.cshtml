@model IEnumerable<TeamI.Models.INSPECTIONDETAILS>

@{
    ViewBag.Title = "Index";
}

<h2>Corrective Problems</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AreaEquipment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.INSPECTION.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USER.firstName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AreaEquipment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.INSPECTION.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USER.firstName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
